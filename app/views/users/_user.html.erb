<tr>
  <td>
    <%= user.id %>
  </td>
  <td>
    <%= user.user_name %>
  </td>
  <td>
    <% user.teachers.each do |teacher| %>
      <%= teacher.lessons.map(&:lesson_name).join(', ') %>
    <% end %>
  </td>
  <td>
    <% user.teachers.each do |teacher| %>
      <%= teacher.subjects.map(&:subject_name).join(', ') %>
    <% end %>
  </td>
</tr>
